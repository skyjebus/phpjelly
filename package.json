{
  "name": "phpjelly",
  "version": "1.0.0",
  "description": "Heroku PHP Jelly Stack",
  "scripts": {
    "build": "postcss ./public/css/tailwind.css -o ./public/css/styles.css --verbose",
    "watch": "watch 'yarn run build' ./public & docker-compose up -d & yarn generate-robot -r & yarn sitemap-generator -f ./public/sitemap.xml https://bananas.one &> /dev/null",
    "images": "cp ./img-magic.sh ./public/images/src; cd ./public/images/src; ./img-magic.sh; rm ./img-magic.sh; cd ../../..",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuelviews/phpjelly.git"
  },
  "author": "@SkyJebus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fuelviews/phpjelly/issues"
  },
  "homepage": "https://github.com/fuelviews/phpjelly#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.5",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.17",
    "tailwindcss-textshadow": "^2.1.3",
    "watch": "^1.0.2",
    "yarn-audit-fix": "^9.0.10"
  },
  "dependencies": {
    "@ruhith/generate-robots-text": "^1.0.1",
    "sitemap-generator-cli": "^7.5.0",
    "yarn": "^1.22.17",
    "@snyk/protect": "latest"
  },
  "snyk": true
}
